<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://ws.voidstar.sjsu.edu/soap/city"
	xmlns:dao="http://ws.voidstar.sjsu.edu/dao"
	targetNamespace="http://ws.voidstar.sjsu.edu/soap/city">

    <xs:import namespace="http://ws.voidstar.sjsu.edu/dao" schemaLocation="../dao/Dao.xsd"/>

	<xs:element name="getCityById" type="tns:getCityById" />
	<xs:element name="getCityByIdResponse" type="tns:getCityByIdResponse" />

	<xs:element name="getCityByCountry" type="tns:getCityByCountry" />
	<xs:element name="getCityByCountryResponse" type="tns:getCityByCountryResponse" />

	<xs:element name="getCityByName" type="tns:getCityByName" />
	<xs:element name="getCityByNameResponse" type="tns:getCityByNameResponse" />
	
	<xs:complexType name="getCityById">
		<xs:sequence>
			<xs:element name="cityId" type="xs:int" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="getCityByIdResponse">
		<xs:sequence>
			<xs:element ref="dao:City" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="getCityByCountry">
		<xs:sequence>
			<xs:element ref="dao:Country" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="getCityByCountryResponse">
		<xs:sequence>
			<xs:element ref="dao:Cities" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="getCityByName">
		<xs:sequence>
			<xs:element name="cityName" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
		<xs:complexType name="getCityByNameResponse">
		<xs:sequence>
			<xs:element ref="dao:Cities" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
</xs:schema>
