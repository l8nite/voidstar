<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" 
	xmlns="http://ws.voidstar.sjsu.edu"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://ws.voidstar.sjsu.edu/soap/language"
	xmlns:dao="http://ws.voidstar.sjsu.edu/dao"
	targetNamespace="http://ws.voidstar.sjsu.edu/soap/language"
	>
		<xs:import namespace="http://ws.voidstar.sjsu.edu/dao" schemaLocation="../dao/Dao.xsd" />
		 
		<xs:element name="getLanguageById" type="tns:getLanguageById" />
		<xs:element name="getLanguageByIdResponse" type="tns:getLanguageByIdResponse" />
		
		<xs:element name="getLanguageByName" type="tns:getLanguageByName" />
		<xs:element name="getLanguageByNameResponse" type="tns:getLanguageByNameResponse"/>
		
		<xs:element name="getLanguagesByCity" type="tns:getLanguagesByCity" />
		<xs:element name="getLanguagesByCityResponse" type="tns:getLanguagesByCityResponse"/>
		
		<xs:element name="getLanguagesByCountry" type="tns:getLanguagesByCountry" />
		<xs:element name="getLanguagesByCountryResponse" type="tns:getLanguagesByCountryResponse"/>
		
		
		<xs:complexType name="getLanguageById">
			<xs:sequence>
				<xs:element name="id" type="xs:int" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
			
		<xs:complexType name="getLanguageByIdResponse">
			<xs:sequence>
				<xs:element ref="dao:Language" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
		
		<xs:complexType name="getLanguageByName">
			<xs:sequence>
				<xs:element name="language" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
			
		<xs:complexType name="getLanguageByNameResponse">
			<xs:sequence>
				<xs:element ref="dao:Language" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
		
		<xs:complexType name="getLanguagesByCity">
			<xs:sequence>
				<xs:element ref="dao:City" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
			
		<xs:complexType name="getLanguagesByCityResponse">
			<xs:sequence>
				<xs:element ref="dao:Language" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
		
		<xs:complexType name="getLanguagesByCountry">
			<xs:sequence>
				<xs:element ref="dao:Country" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
			
		<xs:complexType name="getLanguagesByCountryResponse">
			<xs:sequence>
				<xs:element ref="dao:Language" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
</xs:schema>
